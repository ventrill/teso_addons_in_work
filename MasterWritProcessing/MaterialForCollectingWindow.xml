<GuiXml>
    <Controls>
        <TopLevelControl name="MWP_MaterialForCollectingWindow" hidden="true" mouseEnabled="true" movable="true" clampedToScreen="true">
            <Anchor point="BOTTOM" relativeTo="GuiRoot" relativePoint="BOTTOM" offsetX="0" offsetY="-128"/>
            <Dimensions x="1200" y="500"/>
            <Controls>
                <Backdrop name="$(parent)BG" inherits="ZO_ThinBackdrop" edgeColor="88FFFFFF"/>
                <Control name="$(parent)Title">
                    <Anchor point="TOPLEFT"/>
                    <Anchor point="TOPRIGHT"/>
                    <Dimensions y="30"/>
                    <Controls>
                        <Label name="$(parent)Label" font="ZoFontGameLarge" color="CCAA1A" wrapMode="ELLIPSIS" mouseEnabled="true" text="Materials For Collecting">
                            <Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="17" offsetY="7"/>
                            <Dimensions x="150" y="28"/>
                        </Label>
                        <Button name="$(parent)LoadSavedButton" font="ZoFontGameLarge" color="CCAA1A" wrapMode="ELLIPSIS" mouseEnabled="true" text="Load Saved" inherits="ZO_DefaultButton">
                            <Anchor point="TOPLEFT" relativeTo="$(parent)Label" relativePoint="TOPRIGHT" offsetX="10"/>
                            <Dimensions x="175" y="28"/>
                            <OnClicked>MasterWritProcessing.showMaterialsForProcessingBySaved()</OnClicked>
                        </Button>
                        <Button name="$(parent)LoadActualButton" font="ZoFontGameLarge" color="CCAA1A" wrapMode="ELLIPSIS" mouseEnabled="true" text="Load Actual" inherits="ZO_DefaultButton">
                            <Anchor point="TOPLEFT" relativeTo="$(parent)LoadSavedButton" relativePoint="TOPRIGHT" offsetX="10"/>
                            <Dimensions x="175" y="28"/>
                            <OnClicked>MasterWritProcessing.showMaterialsForProcessingByActual()</OnClicked>
                        </Button>


                        <Button name="$(parent)Close">
                            <Anchor point="TOPRIGHT" offsetX="-5" offsetY="5"/>
                            <OnClicked>MasterWritProcessing.toggleMaterialForCollectingWindow()</OnClicked>
                            <Dimensions x="25" y="25"/>
                            <Textures normal="/esoui/art/buttons/decline_up.dds" pressed="/esoui/art/buttons/decline_down.dds" mouseOver="/esoui/art/buttons/decline_over.dds"/>
                        </Button>
                    </Controls>
                </Control>
                <Control name="$(parent)Headers">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Title" relativePoint="BOTTOMLEFT" offsetX="15" offsetY="20"/>
                    <Dimensions y="32"/>
                    <Controls>
                        <Control name="$(parent)MaterialName" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self,"MaterialName","MaterialName",ZO_SORT_ORDER_UP,TEXT_ALIGN_CENTER,"ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT"/>
                            <Dimensions x="332" y="32"/>
                        </Control>
                        <Control name="$(parent)forMasterCraft" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self,"forMaster","forMasterCraft",ZO_SORT_ORDER_UP,TEXT_ALIGN_CENTER,"ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)MaterialName" relativePoint="TOPRIGHT"/>
                            <Dimensions x="125" y="32"/>
                        </Control>
                        <Control name="$(parent)itemReserve" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self,"itemReserve","itemReserve",ZO_SORT_ORDER_UP,TEXT_ALIGN_CENTER,"ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)forMasterCraft" relativePoint="TOPRIGHT"/>
                            <Dimensions x="125" y="32"/>
                        </Control>
                        <Control name="$(parent)dailyReserve" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self,"dailyReserve","dailyReserve",ZO_SORT_ORDER_UP,TEXT_ALIGN_CENTER,"ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)itemReserve" relativePoint="TOPRIGHT"/>
                            <Dimensions x="125" y="32"/>
                        </Control>
                        <Control name="$(parent)atAll" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self,"atAll","atAll",ZO_SORT_ORDER_UP,TEXT_ALIGN_CENTER,"ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)dailyReserve" relativePoint="TOPRIGHT"/>
                            <Dimensions x="125" y="32"/>
                        </Control>
                        <Control name="$(parent)currentCount" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self,"current","currentCount",ZO_SORT_ORDER_DOWN,TEXT_ALIGN_CENTER,"ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)atAll" relativePoint="TOPRIGHT"/>
                            <Dimensions x="125" y="32"/>
                        </Control>
                        <Control name="$(parent)toCollect" inherits="ZO_SortHeader">
                            <OnInitialized>
                                ZO_SortHeader_Initialize(self,"toCollect","toCollect",ZO_SORT_ORDER_DOWN,TEXT_ALIGN_CENTER,"ZoFontGameLargeBold")
                            </OnInitialized>
                            <Anchor point="TOPLEFT" relativeTo="$(parent)currentCount" relativePoint="TOPRIGHT"/>
                            <Dimensions x="125" y="32"/>
                        </Control>
                    </Controls>
                </Control>
                <Control name="$(parent)List" inherits="ZO_ScrollList">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Headers" relativePoint="BOTTOMLEFT"/>
                    <Anchor point="BOTTOMRIGHT" offsetX="-12" offsetY="-12"/>
                </Control>
            </Controls>
        </TopLevelControl>

        <Button name="MWP_MaterialForCollectingWindowUnitList" virtual="true">
            <Dimensions y="30"/>
            <Anchor point="RIGHT" relativeTo="$(parent)"/>
            <Controls>
                <Texture name="$(parent)MaterialIcon" textureFile="">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)"/>
                    <Dimensions x="32" y="32"/>
                </Texture>
                <Label name="$(parent)MaterialName" font="ZoFontWinH4" wrapMode="ELLIPSIS">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)MaterialIcon" relativePoint="TOPRIGHT"/>
                    <Dimensions x="300" y="32"/>
                </Label>
                <Label name="$(parent)forMasterCraft" font="ZoFontWinH4" wrapMode="ELLIPSIS">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)MaterialName" relativePoint="TOPRIGHT"/>
                    <Dimensions x="125" y="32"/>
                </Label>
                <Label name="$(parent)itemReserve" font="ZoFontWinH4" wrapMode="ELLIPSIS">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)forMasterCraft" relativePoint="TOPRIGHT"/>
                    <Dimensions x="125" y="32"/>
                </Label>
                <Label name="$(parent)dailyReserve" font="ZoFontWinH4" wrapMode="ELLIPSIS">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)itemReserve" relativePoint="TOPRIGHT"/>
                    <Dimensions x="125" y="32"/>
                </Label>
                <Label name="$(parent)atAll" font="ZoFontWinH4" wrapMode="ELLIPSIS">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)dailyReserve" relativePoint="TOPRIGHT"/>
                    <Dimensions x="125" y="32"/>
                </Label>
                <Label name="$(parent)currentCount" font="ZoFontWinH4" wrapMode="ELLIPSIS">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)atAll" relativePoint="TOPRIGHT"/>
                    <Dimensions x="125" y="32"/>
                </Label>
                <Label name="$(parent)toCollect" font="ZoFontWinH4" wrapMode="ELLIPSIS">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)currentCount" relativePoint="TOPRIGHT"/>
                    <Dimensions x="125" y="32"/>
                </Label>
            </Controls>
        </Button>
    </Controls>

</GuiXml>